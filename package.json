{
  "name": "hmi-web",
  "version": "1.0.0",
  "description": "hmi-web by koa2 + MongoDB + GraphQL + typescript + fabricjs",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc & node dist/index.js",
    "debugger": "nodemon --watch ./src -e ts,tsx --exec node --inspect -r ts-node/register ./src/server.ts",
    "watch-serve": "nodemon --watch ./src -e ts,tsx --exec ts-node ./src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZjpNode/hmi-web.git"
  },
  "keywords": [
    "koa2",
    "MongoDB",
    "GraphQL",
    "fabricjs",
    "typescript"
  ],
  "author": "zjp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZjpNode/hmi-web/issues"
  },
  "homepage": "https://github.com/ZjpNode/hmi-web#readme",
  "dependencies": {
    "koa": "^2.7.0",
    "koa-router": "^7.4.0"
  },
  "devDependencies": {
    "@types/koa": "^2.0.49",
    "@types/koa-router": "^7.0.42",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
